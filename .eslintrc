{
  "extends": [
    "plugin:prettier/recommended",
    "next",
    "next/core-web-vitals"
  ],
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true,
    "jest": true,
    "node": true
  },
  "rules": {
    "jsx-a11y/href-no-hash": ["off"],
    "react/jsx-filename-extension": ["warn", { "extensions": [".js", ".jsx"] }],
    "prettier/prettier": ["error", {
      "endOfLine": "auto"
    }, {
      "usePrettierrc": true
    }]
  }
}
